@import "../../app.wxss";

.container {
  position: relative;
  width: 100%;
  height: 1334rpx;
}

.title {
  position: absolute;
  left: 50%;
  top: 40%;
  transform: translate(-50%,-40%);
  font-size: 32rpx;
}

.button {
  position: absolute !important;
  left: 50% !important;
  top: 50% !important;
  transform: translate(-50%,-50%) !important;
}